defaults.pcm.rate_converter "samplerate_best"
defaults.pcm.format S16_LE
defaults.pcm.rate 44100

pcm.!spdif {
  type hw
  card 0
  device 0
}

pcm.!default {
  type plug
  slave.pcm "spdif"
}

pcm.!surround51 {
    type vdownmix
    slave.pcm "default"
}
pcm.!surround40 {
    type vdownmix
    slave.pcm "default"
}
